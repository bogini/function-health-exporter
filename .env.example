# Function Health CLI Configuration
# Copy this file to .env and configure as needed

# Credentials (optional - will prompt if not provided)
FUNCTION_HEALTH_EMAIL=your.email@example.com
# Note: Password is not stored in env for security - will always prompt

# Output Configuration
OUTPUT_DIR=function-health-export

# API Configuration
RETRY_ATTEMPTS=3
RETRY_DELAY=1000
RATE_LIMIT=500
TOKEN_REFRESH_BUFFER=300
MAX_INDIVIDUAL_BIOMARKERS=0  # 0 = no limit, fetch all biomarkers

# Logging
LOG_LEVEL=info
VERBOSE=false
QUIET=false
DEBUG=false